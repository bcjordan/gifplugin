{
  "name": "InstaGif",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Find links to gifs instantly.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "http://ajax.googleapis.com/",
    "http://api.flickr.com/",
    "http://reddit.com/",
    "http://tumblr.com/",
    "http://senorgif.memebase.com/"
  ]
}
